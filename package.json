{
  "name": "victron-vrm-api",
  "version": "0.3.6",
  "description": "Interface with the Victron Energy VRM API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "standard --fix"
  },
  "keywords": [
    "node-red",
    "victron",
    "vrm",
    "api"
  ],
  "author": "Dirk-Jan Faber <dfaber@victronenergy.com>",
  "license": "MIT",
  "files": [
    "examples",
    "src",
    "LICENSE"
  ],
  "node-red": {
    "nodes": {
      "config-vrm-api": "./src/nodes/config-vrm-api.js",
      "vrm-api": "./src/nodes/vrm-api.js"
    },
    "version": ">=3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dirkjanfaber/victron-vrm-api.git"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-curlirize": "^1.3.7",
    "debug": "^4.4.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "dotenv": "^16.6.1",
    "jest": "^29.7.0",
    "node-red": "^3.1.0",
    "node-red-node-test-helper": "^0.3.4",
    "standard": "^17.1.0"
  },
  "engines": {
    "node": ">=14.17.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  }
}
